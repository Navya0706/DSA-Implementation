public class Main {
    public static int linearSearch(int[] array, int target) {
        int n=array.length;
        for (int i = 0; i <n ; i++) {
            if (array[i] == target) {
                return i;
            }
        }
        return -1;
    }

    public static void main(String[] args) {
        int[] array = {10, 20, 30, 40, 50};
        int target = 30;
        int result = linearSearch(array, target);
        System.out.println(result);
    }
}
